<m:Page_
    x:Class="CMScoutIntrinsic.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="using:CMScoutIntrinsic"
    xmlns:i="using:Microsoft.Xaml.Interactivity"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Page.TopAppBar>
        <CommandBar
            HorizontalContentAlignment="Stretch"
            IsDynamicOverflowEnabled="False"
            x:Name="CommandBar">
            <CommandBar.Content>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <m:AppBarButton
                        Grid.Row="0" Grid.Column="0"
                        Icon="OpenFile" Label="Open"
                        IsCompact="{Binding ElementName=CommandBar, Path=IsOpen, Converter={StaticResource InverseBooleanConverter}}"
                        Command="{Binding Open}">
                        <FlyoutBase.AttachedFlyout>
                            <Flyout>
                                <Grid
                                    DataContext="{Binding OpenViewModel}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <ListView
                                        Grid.Row="0" Grid.Column="0"
                                        ItemsSource="{Binding MruFiles}"
                                        SelectionMode="None"
                                        IsItemClickEnabled="True">
                                        <ListView.ItemContainerStyle>
                                            <Style TargetType="ListViewItem">
                                                <Setter Property="FocusVisualPrimaryThickness"   Value="0" />
                                                <Setter Property="FocusVisualSecondaryThickness" Value="0" />
                                            </Style>
                                        </ListView.ItemContainerStyle>
                                        <ListView.ItemTemplate>
                                            <DataTemplate>
                                                <Grid
                                                    Height="48">
                                                    <TextBlock
                                                        Text="{Binding Path}"
                                                        VerticalAlignment="Center" />
                                                </Grid>
                                            </DataTemplate>
                                        </ListView.ItemTemplate>
                                        <i:Interaction.Behaviors>
                                            <m:ListViewBaseBehavior ItemClickCommand="{Binding OpenOldFile}" />
                                        </i:Interaction.Behaviors>
                                    </ListView>
                                    <Button
                                        Grid.Row="1" Grid.Column="0"
                                        Content="Open New File"
                                        Command="{Binding OpenNewFile}"
                                        HorizontalAlignment="Center" VerticalAlignment="Center"
                                        Margin="0,16,0,0"/>
                                </Grid>
                            </Flyout>
                        </FlyoutBase.AttachedFlyout>
                        <i:Interaction.Behaviors>
                            <m:FlyoutBehavior IsOpened="{Binding OpenViewModel.IsOpened, Mode=TwoWay}" />
                        </i:Interaction.Behaviors>
                    </m:AppBarButton>
                    <m:AppBarButton
                        Grid.Row="0" Grid.Column="1"
                        Icon="Refresh" Label="Reload"
                        IsCompact="{Binding ElementName=CommandBar, Path=IsOpen, Converter={StaticResource InverseBooleanConverter}}"
                        Command="{Binding Reload}">
                        <m:AppBarButton.Opacity>
                            <Binding Path="OpenViewModel.OpenedFile">
                                <Binding.Converter>
                                    <m:ConditionConverter Condition="==" OnRight="{x:Null}" IfTrue="0.5" IfFalse="1.0" />
                                </Binding.Converter>
                            </Binding>
                        </m:AppBarButton.Opacity>
                    </m:AppBarButton>
                    <m:AppBarButton
                        Grid.Row="0" Grid.Column="2"
                        Icon="Filter" Label="Filters"
                        IsCompact="{Binding ElementName=CommandBar, Path=IsOpen, Converter={StaticResource InverseBooleanConverter}}"
                        Command="{Binding Filters}"
                        AllowFocusOnInteraction="True">
                        <m:AppBarButton.Opacity>
                            <Binding Path="OpenViewModel.OpenedFile">
                                <Binding.Converter>
                                    <m:ConditionConverter Condition="==" OnRight="{x:Null}" IfTrue="0.5" IfFalse="1.0" />
                                </Binding.Converter>
                            </Binding>
                        </m:AppBarButton.Opacity>
                        <FlyoutBase.AttachedFlyout>
                            <Flyout>
                                <Flyout.FlyoutPresenterStyle>
                                    <Style TargetType="FlyoutPresenter">
                                        <Setter Property="MaxWidth" Value="1022" />
                                    </Style>
                                </Flyout.FlyoutPresenterStyle>
                                <Grid
                                    DataContext="{Binding FiltersViewModel}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="508" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="200" />
                                        <ColumnDefinition Width="788" />
                                    </Grid.ColumnDefinitions>
                                    <ListView
                                        Grid.Row="0" Grid.Column="0"
                                        ItemsSource="{Binding Filters}"
                                        SelectedItem="{Binding SelectedFilter, Mode=TwoWay}"
                                        SelectionMode="Single"
                                        Background="Gainsboro">
                                        <ListView.ItemContainerStyle>
                                            <Style TargetType="ListViewItem">
                                                <Setter Property="FocusVisualPrimaryThickness"   Value="0" />
                                                <Setter Property="FocusVisualSecondaryThickness" Value="0" />
                                            </Style>
                                        </ListView.ItemContainerStyle>
                                        <ListView.ItemContainerTransitions>
                                            <TransitionCollection />
                                        </ListView.ItemContainerTransitions>
                                        <ListView.ItemTemplate>
                                            <DataTemplate>
                                                <Grid
                                                    Height="48">
                                                    <TextBlock
                                                        Text="{Binding Name}"
                                                        TextWrapping="Wrap"
                                                        VerticalAlignment="Center" />
                                                </Grid>
                                            </DataTemplate>
                                        </ListView.ItemTemplate>
                                    </ListView>
                                    <Grid
                                        Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="0"
                                            Icon="Page2" Label="New"
                                            Command="{Binding New}"
                                            Width="72" />
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="1"
                                            Icon="Copy" Label="Copy"
                                            Command="{Binding Copy}"
                                            Width="72" />
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="2"
                                            Icon="Delete" Label="Delete"
                                            Command="{Binding Delete}"
                                            Width="72" />
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="3"
                                            Icon="Up" Label="Move Up"
                                            Command="{Binding MoveUp}"
                                            Width="72"
                                            Margin="32,0,0,0" />
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="4"
                                            Label="Move Down"
                                            Command="{Binding MoveDown}"
                                            Width="72">
                                            <AppBarButton.Icon>
                                                <SymbolIcon Symbol="Up" RenderTransformOrigin="0.5,0.5">
                                                    <SymbolIcon.RenderTransform>
                                                        <ScaleTransform ScaleY="-1" />
                                                    </SymbolIcon.RenderTransform>
                                                </SymbolIcon>
                                            </AppBarButton.Icon>
                                        </AppBarButton>
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="5"
                                            Icon="Clear" Label="Reset"
                                            Command="{Binding Reset}"
                                            Width="72"
                                            Margin="32,0,0,0" />
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="6"
                                            Icon="Clear" Label="Reset Tab"
                                            Command="{Binding ResetTab}"
                                            Width="72" />
                                        <AppBarButton
                                            Grid.Row="0" Grid.Column="8"
                                            Icon="Accept" Label="Apply"
                                            Command="{Binding Apply}"
                                            Width="72" />
                                    </Grid>
                                    <Grid
                                        Grid.Row="0" Grid.Column="1"
                                        Visibility="{Binding SelectedFilter, Converter={StaticResource NullToVisibilityConverter}}">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <TextBox
                                            Grid.Row="0" Grid.Column="0"
                                            Text="{Binding SelectedFilter.Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                            HorizontalAlignment="Stretch" VerticalAlignment="Center" TextAlignment="Center"
                                            Margin="16,0,0,0" />
                                        <Pivot
                                            Grid.Row="1" Grid.Column="0"
                                            SelectedIndex="{Binding SelectedContentIndex, Mode=TwoWay}">
                                            <Pivot.ItemContainerStyle>
                                                <Style TargetType="PivotItem">
                                                    <Setter Property="Margin" Value="16,0,0,0" />
                                                </Style>
                                            </Pivot.ItemContainerStyle>
                                            <PivotItem
                                                Header="General">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>
                                                    
                                                    <StackPanel
                                                        Grid.Row="0" Grid.Column="0"
                                                        Orientation="Vertical">

                                                        <m:GroupBoxControl
                                                            Header="Name">
                                                            <TextBox
                                                                Text="{Binding SelectedFilter.PlayerName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                Width="256"
                                                                Margin="4" /> 
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Nation"
                                                            Margin="0,8,0,0">
                                                            <Button
                                                                Content="{Binding SelectedFilter.NationGroupName, TargetNullValue={StaticResource AnyNationGroupName}}"
                                                                Width="256"
                                                                Margin="4" >
                                                                <Button.Flyout>
                                                                    <Flyout>
                                                                        <Grid>
                                                                            <Grid.RowDefinitions>
                                                                                <RowDefinition Height="Auto" />
                                                                                <RowDefinition Height="600" />
                                                                            </Grid.RowDefinitions>
                                                                            <Grid.ColumnDefinitions>
                                                                                <ColumnDefinition Width="300" />
                                                                            </Grid.ColumnDefinitions>
                                                                            <TextBox
                                                                                Grid.Row="0" Grid.Column="0"
                                                                                Text="{Binding NationGroupSearchString, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                Margin="24,0,24,8" />
                                                                            <ListView
                                                                                Grid.Row="1" Grid.Column="0"
                                                                                ItemsSource="{Binding NationGroups}"
                                                                                SelectedItem="{Binding SelectedNationGroup, Mode=TwoWay}"
                                                                                SelectionMode="Single"
                                                                                IsItemClickEnabled="True">
                                                                                <ListView.ItemContainerStyle>
                                                                                    <Style TargetType="ListViewItem">
                                                                                        <Setter Property="FocusVisualPrimaryThickness"   Value="0"    />
                                                                                        <Setter Property="FocusVisualSecondaryThickness" Value="0"    />
                                                                                        <Setter Property="Margin"                        Value="24,0" />
                                                                                    </Style>
                                                                                </ListView.ItemContainerStyle>
                                                                                <ListView.ItemTemplate>
                                                                                    <DataTemplate>
                                                                                        <Grid
                                                                                            Height="48">
                                                                                            <TextBlock
                                                                                                Text="{Binding Name, TargetNullValue={StaticResource AnyNationGroupName}}"
                                                                                                VerticalAlignment="Center" />
                                                                                        </Grid>
                                                                                    </DataTemplate>
                                                                                </ListView.ItemTemplate>
                                                                                <i:Interaction.Behaviors>
                                                                                    <m:ListViewBaseBehavior
                                                                                        ItemClickCommand="{Binding NationGroupActivated}"
                                                                                        ScrollFunction="{Binding ScrollToNationGroup, Mode=TwoWay}" />
                                                                                </i:Interaction.Behaviors>
                                                                            </ListView>
                                                                        </Grid>
                                                                    </Flyout>
                                                                </Button.Flyout>
                                                                <i:Interaction.Behaviors>
                                                                    <m:FlyoutBehavior IsOpened="{Binding AreNationGroupsOpened, Mode=TwoWay}" />
                                                                </i:Interaction.Behaviors>
                                                            </Button>
                                                        </m:GroupBoxControl>
                                                    
                                                        <m:GroupBoxControl
                                                            Header="Club"
                                                            Margin="0,8,0,0">
                                                            <Button
                                                                Content="{Binding SelectedFilter.ClubName, TargetNullValue={StaticResource AnyClubName}}"
                                                                Width="256"
                                                                Margin="4" >
                                                                <Button.Flyout>
                                                                    <Flyout>
                                                                        <Grid>
                                                                            <Grid.RowDefinitions>
                                                                                <RowDefinition Height="Auto" />
                                                                                <RowDefinition Height="600" />
                                                                            </Grid.RowDefinitions>
                                                                            <Grid.ColumnDefinitions>
                                                                                <ColumnDefinition Width="300" />
                                                                            </Grid.ColumnDefinitions>
                                                                            <TextBox
                                                                                Grid.Row="0" Grid.Column="0"
                                                                                Text="{Binding ClubSearchString, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                Margin="24,0,24,8" />
                                                                            <ListView
                                                                                Grid.Row="1" Grid.Column="0"
                                                                                ItemsSource="{Binding Clubs}"
                                                                                SelectedItem="{Binding SelectedClub, Mode=TwoWay}"
                                                                                SelectionMode="Single"
                                                                                IsItemClickEnabled="True">
                                                                                <ListView.ItemContainerStyle>
                                                                                    <Style TargetType="ListViewItem">
                                                                                        <Setter Property="FocusVisualPrimaryThickness"   Value="0"    />
                                                                                        <Setter Property="FocusVisualSecondaryThickness" Value="0"    />
                                                                                        <Setter Property="Margin"                        Value="24,0" />
                                                                                    </Style>
                                                                                </ListView.ItemContainerStyle>
                                                                                <ListView.ItemTemplate>
                                                                                    <DataTemplate>
                                                                                        <Grid
                                                                                            Height="48">
                                                                                            <TextBlock
                                                                                                Text="{Binding Name, TargetNullValue={StaticResource AnyClubName}}"
                                                                                                VerticalAlignment="Center" />
                                                                                        </Grid>
                                                                                    </DataTemplate>
                                                                                </ListView.ItemTemplate>
                                                                                <i:Interaction.Behaviors>
                                                                                    <m:ListViewBaseBehavior
                                                                                        ItemClickCommand="{Binding ClubActivated}"
                                                                                        ScrollFunction="{Binding ScrollToClub, Mode=TwoWay}" />
                                                                                </i:Interaction.Behaviors>
                                                                            </ListView>
                                                                        </Grid>
                                                                    </Flyout>
                                                                </Button.Flyout>
                                                                <i:Interaction.Behaviors>
                                                                    <m:FlyoutBehavior IsOpened="{Binding AreClubsOpened, Mode=TwoWay}" />
                                                                </i:Interaction.Behaviors>
                                                            </Button>
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Age"
                                                            Margin="0,8,0,0">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="16" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <m:NumberPickerControl
                                                                    Grid.Row="0" Grid.Column="0"
                                                                    From="0" To="{Binding MaxAge}"
                                                                    SelectedNumber="{Binding SelectedFilter.AgeFrom, Mode=TwoWay}" />
                                                                <m:NumberPickerControl
                                                                    Grid.Row="0" Grid.Column="2"
                                                                    From="0" To="{Binding MaxAge}"
                                                                    SelectedNumber="{Binding SelectedFilter.AgeTo, Mode=TwoWay}" />
                                                            </Grid>
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Current Ability"
                                                            Margin="0,8,0,0">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="16" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <m:NumberPickerControl
                                                                    Grid.Row="0" Grid.Column="0"
                                                                    From="1" To="200"
                                                                    SelectedNumber="{Binding SelectedFilter.CAFrom, Mode=TwoWay}" />
                                                                <m:NumberPickerControl
                                                                    Grid.Row="0" Grid.Column="2"
                                                                    From="1" To="200"
                                                                    SelectedNumber="{Binding SelectedFilter.CATo, Mode=TwoWay}" />
                                                            </Grid>
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Potential Ability"
                                                            Margin="0,8,0,0">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="16" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <m:NumberPickerControl
                                                                    Grid.Row="0" Grid.Column="0"
                                                                    From="1" To="200"
                                                                    SelectedNumber="{Binding SelectedFilter.PAFrom, Mode=TwoWay}" />
                                                                <m:NumberPickerControl
                                                                    Grid.Row="0" Grid.Column="2"
                                                                    From="1" To="200"
                                                                    SelectedNumber="{Binding SelectedFilter.PATo, Mode=TwoWay}" />
                                                            </Grid>
                                                        </m:GroupBoxControl>

                                                    </StackPanel>
                                                    
                                                    <StackPanel
                                                        Grid.Row="0" Grid.Column="1"
                                                        Orientation="Vertical"
                                                        Margin="32,0,0,0">

                                                        <m:GroupBoxControl
                                                            Header="Side">
                                                            <ItemsControl
                                                                ItemsSource="{Binding SelectedFilter.Sides}">
                                                                <ItemsControl.ItemsPanel>
                                                                    <ItemsPanelTemplate>
                                                                        <StackPanel Orientation="Vertical" />
                                                                    </ItemsPanelTemplate>
                                                                </ItemsControl.ItemsPanel>
                                                                <ItemsControl.ItemTemplate>
                                                                    <DataTemplate>
                                                                        <CheckBox
                                                                            Content="{Binding Name}"
                                                                            IsChecked="{Binding IsChecked, Mode=TwoWay}"
                                                                            Width="256"
                                                                            Margin="4,0">
                                                                        </CheckBox>
                                                                    </DataTemplate>
                                                                </ItemsControl.ItemTemplate>
                                                            </ItemsControl>
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Position"
                                                            Margin="0,8,0,0">
                                                            <ItemsControl
                                                                ItemsSource="{Binding SelectedFilter.Positions}">
                                                                <ItemsControl.ItemsPanel>
                                                                    <ItemsPanelTemplate>
                                                                        <StackPanel Orientation="Vertical" />
                                                                    </ItemsPanelTemplate>
                                                                </ItemsControl.ItemsPanel>
                                                                <ItemsControl.ItemTemplate>
                                                                    <DataTemplate>
                                                                        <CheckBox
                                                                            Content="{Binding Name}"
                                                                            IsChecked="{Binding IsChecked, Mode=TwoWay}"
                                                                            Width="256"
                                                                            Margin="4,0">
                                                                        </CheckBox>
                                                                    </DataTemplate>
                                                                </ItemsControl.ItemTemplate>
                                                            </ItemsControl>
                                                        </m:GroupBoxControl>

                                                    </StackPanel>
                                                </Grid>
                                            </PivotItem>
                                            <PivotItem
                                                Header="Contract">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <StackPanel
                                                        Grid.Row="0" Grid.Column="0"
                                                        Orientation="Vertical">

                                                        <m:GroupBoxControl
                                                            Header="Value">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <TextBox
                                                                    Grid.Row="0" Grid.Column="0"
                                                                    Text="{Binding SelectedFilter.ValueFrom, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                    InputScope="Number"
                                                                    Width="144">
                                                                    <i:Interaction.Behaviors>
                                                                        <m:NumericTextBoxBehavior />
                                                                    </i:Interaction.Behaviors>
                                                                </TextBox>
                                                                <TextBlock
                                                                    Grid.Row="0" Grid.Column="1"
                                                                    Text="&#x00A3;"
                                                                    HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                                                                    Width="16"
                                                                    Margin="8,0,8,0" />
                                                                <TextBox
                                                                    Grid.Row="0" Grid.Column="2"
                                                                    Text="{Binding SelectedFilter.ValueTo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                    InputScope="Number"
                                                                    Width="144">
                                                                    <i:Interaction.Behaviors>
                                                                        <m:NumericTextBoxBehavior />
                                                                    </i:Interaction.Behaviors>
                                                                </TextBox>
                                                            </Grid>
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Wage"
                                                            Margin="0,8,0,0">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <TextBox
                                                                    Grid.Row="0" Grid.Column="0"
                                                                    Text="{Binding SelectedFilter.WageFrom, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                    InputScope="Number"
                                                                    Width="112">
                                                                    <i:Interaction.Behaviors>
                                                                        <m:NumericTextBoxBehavior />
                                                                    </i:Interaction.Behaviors>
                                                                </TextBox>
                                                                <TextBlock
                                                                    Grid.Row="0" Grid.Column="1"
                                                                    Text="&#x00A3; per week"
                                                                    HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                                                                    Width="80"
                                                                    Margin="8,0,8,0" />
                                                                <TextBox
                                                                    Grid.Row="0" Grid.Column="2"
                                                                    Text="{Binding SelectedFilter.WageTo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                    InputScope="Number"
                                                                    Width="112">
                                                                    <i:Interaction.Behaviors>
                                                                        <m:NumericTextBoxBehavior />
                                                                    </i:Interaction.Behaviors>
                                                                </TextBox>
                                                            </Grid>
                                                        </m:GroupBoxControl>

                                                        <m:GroupBoxControl
                                                            Header="Contract"
                                                            Margin="0,8,0,0">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <CheckBox Grid.Row="0" Grid.Column="0" Content="Free Transfer"            IsChecked="{Binding SelectedFilter.IsFreeTransfer,        Mode=TwoWay}" Width="320" />
                                                                <CheckBox Grid.Row="1" Grid.Column="0" Content="Expired"                  IsChecked="{Binding SelectedFilter.IsContractExpired,     Mode=TwoWay}" />
                                                                <CheckBox Grid.Row="2" Grid.Column="0" Content="Expiring"                 IsChecked="{Binding SelectedFilter.IsContractExpiring,    Mode=TwoWay}" />
                                                                <CheckBox Grid.Row="3" Grid.Column="0" Content="Unprotected"              IsChecked="{Binding SelectedFilter.IsContractUnprotected, Mode=TwoWay}" />
                                                                <CheckBox Grid.Row="4" Grid.Column="0" Content="Leaving On Bosman Ruling" IsThreeState="True" Margin="0,32,0,0">
                                                                    <i:Interaction.Behaviors>
                                                                        <m:ThreeStateCheckBoxBehavior IsChecked="{Binding SelectedFilter.IsLeavingOnBosman, Mode=TwoWay}" />
                                                                    </i:Interaction.Behaviors>
                                                                </CheckBox>
                                                                <CheckBox Grid.Row="5" Grid.Column="0" Content="Transfer Arranged" IsThreeState="True">
                                                                    <i:Interaction.Behaviors>
                                                                        <m:ThreeStateCheckBoxBehavior IsChecked="{Binding SelectedFilter.IsTransferArranged, Mode=TwoWay}" />
                                                                    </i:Interaction.Behaviors>
                                                                </CheckBox>
                                                            </Grid>
                                                        </m:GroupBoxControl>

                                                    </StackPanel>

                                                    <StackPanel
                                                        Grid.Row="0" Grid.Column="1"
                                                        Orientation="Vertical"
                                                        Margin="32,0,0,0">

                                                        <m:GroupBoxControl
                                                            Header="Transfer Status">
                                                            <Grid
                                                                Margin="4,0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="Auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <CheckBox Grid.Row="0" Grid.Column="0" Content="{Binding TransferStatuses[1].Name}" IsChecked="{Binding SelectedFilter.IsTransferListedByClub,    Mode=TwoWay}" />
