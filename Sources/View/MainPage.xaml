                            <TextBlock Grid.Row="0"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding RightFoot}"           />
                            <TextBlock Grid.Row="1"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Left Foot"                     />
                            <TextBlock Grid.Row="1"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding LeftFoot}"            />

                            <TextBlock Grid.Row="3"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Goalkeeper"                    />
                            <TextBlock Grid.Row="3"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Goalkeeper}"          />
                            <TextBlock Grid.Row="4"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Sweeper"                       />
                            <TextBlock Grid.Row="4"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Sweeper}"             />
                            <TextBlock Grid.Row="5"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Defender"                      />
                            <TextBlock Grid.Row="5"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Defender}"            />
                            <TextBlock Grid.Row="6"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Defensive Midfielder"          />
                            <TextBlock Grid.Row="6"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding DefensiveMidfielder}" />
                            <TextBlock Grid.Row="7"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Midfielder"                    />
                            <TextBlock Grid.Row="7"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Midfielder}"          />
                            <TextBlock Grid.Row="8"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Attacking Midfielder"          />
                            <TextBlock Grid.Row="8"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding AttackingMidfielder}" />
                            <TextBlock Grid.Row="9"  Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Attacker"                      />
                            <TextBlock Grid.Row="9"  Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Attacker}"            />
                            <TextBlock Grid.Row="10" Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Wing Back"                     />
                            <TextBlock Grid.Row="10" Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding WingBack}"            />
                            <TextBlock Grid.Row="11" Grid.Column="6" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="Free Role"                     />
                            <TextBlock Grid.Row="11" Grid.Column="7" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding FreeRole}"            />
                        </Grid>
                    </Grid>
                </PivotItem>
                <PivotItem
                    Header="Contract">
                    <Grid
                        Visibility="{Binding Contract, Converter={StaticResource NullToVisibilityConverter}}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="16" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="128" />
                            <ColumnDefinition Width="16" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <!-- Contract -->
                        <TextBlock Grid.Row="0" Grid.Column="0" Text="Type" />
                        <TextBlock Grid.Row="0" Grid.Column="2" Text="{Binding Contract.Type, Converter={StaticResource ContractTypeToStringConverter}}" />

                        <TextBlock Grid.Row="1" Grid.Column="0" Text="Wages" />
                        <TextBlock Grid.Row="1" Grid.Column="2" Text="{Binding Contract.Wage, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0} per week'}" />

                        <TextBlock Grid.Row="2" Grid.Column="0" Text="Started" />
                        <TextBlock Grid.Row="2" Grid.Column="2" Text="{Binding Contract.DateStarted, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}{0:d}'}" />

                        <TextBlock Grid.Row="3" Grid.Column="0" Text="Protected" />
                        <TextBlock Grid.Row="3" Grid.Column="2" Text="{Binding Contract.UnprotectedReason, Converter={StaticResource ContractProtectionToStringConverter}}" />

                        <TextBlock Grid.Row="4" Grid.Column="0" Text="Expires" />
                        <TextBlock Grid.Row="4" Grid.Column="2" Text="{Binding Contract.DateEnded, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}{0:d}'}" />

                        <TextBlock Grid.Row="5" Grid.Column="0" Text="Loan Details"                                                     Visibility="{Binding LoanContract, Converter={StaticResource NullToVisibilityConverter}}" />
                        <TextBlock Grid.Row="5" Grid.Column="2" Text="{Binding Converter={StaticResource LoanStatusToStringConverter}}" Visibility="{Binding LoanContract, Converter={StaticResource NullToVisibilityConverter}}" />

                        <TextBlock Grid.Row="6" Grid.Column="0" Text="Squad Status" />
                        <TextBlock Grid.Row="6" Grid.Column="2" Text="{Binding Contract.SquadStatus, Converter={StaticResource SquadStatusToStringConverter}}" />
                        
                        <TextBlock Grid.Row="7" Grid.Column="0" Text="Bonuses" />
                        <StackPanel
                            Grid.Row="7" Grid.Column="2"
                            Orientation="Vertical">
                            <TextBlock Text="None"                                                                                                                                          Visibility="{Binding Contract.HasBonuses,      Converter={StaticResource BooleanToInvertedVisibilityConverter}}" />
                            <TextBlock Text="{Binding Contract.GoalBonus,       Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0} Goal Bonus'}"        Visibility="{Binding Contract.GoalBonus,       Converter={StaticResource BonusToVisibilityConverter}}"           />
                            <TextBlock Text="{Binding Contract.AssistBonus,     Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0} Assist Bonus'}"      Visibility="{Binding Contract.AssistBonus,     Converter={StaticResource BonusToVisibilityConverter}}"           />
                            <TextBlock Text="{Binding Contract.CleanSheetBonus, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0} Clean Sheet Bonus'}" Visibility="{Binding Contract.CleanSheetBonus, Converter={StaticResource BonusToVisibilityConverter}}"           />
                        </StackPanel>

                        <TextBlock Grid.Row="8" Grid.Column="0" Text="Clauses" />
                        <StackPanel
                            Grid.Row="8" Grid.Column="2"
                            Orientation="Vertical">
                            <TextBlock Text="None" Visibility="{Binding Contract.HasRC, Converter={StaticResource BooleanToInvertedVisibilityConverter}}" />
                            <TextBlock Text="{Binding Contract.ReleaseFee, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}Non Promotion Release Clause (&#x00A3;{0:N0})'}" Foreground="{Binding Contract.IsNonPromotionRCActive, Converter={StaticResource ActiveReleaseClauseToBrushConverter}}" Visibility="{Binding Contract.HasNonPromotionRC, Converter={StaticResource BooleanToVisibilityConverter}}" />
                            <TextBlock Text="{Binding Contract.ReleaseFee, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}Minimum Fee Release Clause (&#x00A3;{0:N0})'}"   Foreground="{Binding Contract.IsMinimumFeeRCActive,   Converter={StaticResource ActiveReleaseClauseToBrushConverter}}" Visibility="{Binding Contract.HasMinimumFeeRC,   Converter={StaticResource BooleanToVisibilityConverter}}" />
                            <TextBlock Text="{Binding Contract.ReleaseFee, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}Non Playing Release Clause (&#x00A3;{0:N0})'}"   Foreground="{Binding Contract.IsNonPlayingRCActive,   Converter={StaticResource ActiveReleaseClauseToBrushConverter}}" Visibility="{Binding Contract.HasNonPlayingRC,   Converter={StaticResource BooleanToVisibilityConverter}}" />
                            <TextBlock Text="{Binding Contract.ReleaseFee, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}Relegation Release Clause (&#x00A3;{0:N0})'}"    Foreground="{Binding Contract.IsRelegationRCActive,   Converter={StaticResource ActiveReleaseClauseToBrushConverter}}" Visibility="{Binding Contract.HasRelegationRC,   Converter={StaticResource BooleanToVisibilityConverter}}" />
                            <TextBlock Text="{Binding Contract.ReleaseFee, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}Manager Job Release Clause (&#x00A3;{0:N0})'}"   Foreground="{Binding Contract.IsManagerJobRCActive,   Converter={StaticResource ActiveReleaseClauseToBrushConverter}}" Visibility="{Binding Contract.HasManagerJobRC,   Converter={StaticResource BooleanToVisibilityConverter}}" />
                        </StackPanel>
                        
                        <!-- Transfer -->
                        <TextBlock Grid.Row="10" Grid.Column="0" Text="Availability" />
                        <TextBlock Grid.Row="10" Grid.Column="2" Text="{Binding Contract.TransferStatus, Converter={StaticResource TransferStatusToStringConverter}}" />

                        <TextBlock Grid.Row="11" Grid.Column="0" Text="Value" />
                        <TextBlock Grid.Row="11" Grid.Column="2" Text="{Binding Value_, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0}'}" />

                        <TextBlock Grid.Row="12" Grid.Column="0" Text="2nd Nationality"                   Visibility="{Binding SecondNationNationality, Converter={StaticResource EmptyToVisibilityConverter}}" />
                        <TextBlock Grid.Row="12" Grid.Column="2" Text="{Binding SecondNationNationality}" Visibility="{Binding SecondNationNationality, Converter={StaticResource EmptyToVisibilityConverter}}" />

                        <TextBlock Grid.Row="13" Grid.Column="0" Text="Future" />
                        <TextBlock Grid.Row="13" Grid.Column="2" Text="{Binding Converter={StaticResource FutureToStringConverter}}" Foreground="{StaticResource ActiveReleaseClauseBrush}" />

                    </Grid>
                </PivotItem>
                <PivotItem
                    Header="Ratings">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="48" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        
                        <TextBlock Grid.Row="0"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[0].Name}"                                               />
                        <TextBlock Grid.Row="0"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[0].Value, Converter={StaticResource Double2Converter}}" />
                        <TextBlock Grid.Row="1"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[1].Name}"                                               />
                        <TextBlock Grid.Row="1"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[1].Value, Converter={StaticResource Double2Converter}}" />
                        <TextBlock Grid.Row="2"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[2].Name}"                                               />
                        <TextBlock Grid.Row="2"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[2].Value, Converter={StaticResource Double2Converter}}" />
                        <TextBlock Grid.Row="3"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[3].Name}"                                               />
                        <TextBlock Grid.Row="3"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[3].Value, Converter={StaticResource Double2Converter}}" />
                        <TextBlock Grid.Row="4"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[4].Name}"                                               />
                        <TextBlock Grid.Row="4"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[4].Value, Converter={StaticResource Double2Converter}}" />
                        <TextBlock Grid.Row="5"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[5].Name}"                                               />
                        <TextBlock Grid.Row="5"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[5].Value, Converter={StaticResource Double2Converter}}" />
                        <TextBlock Grid.Row="6"  Grid.Column="0" Style="{StaticResource AttributeNameTextBlockStyle}"  Text="{Binding Ratings[6].Name}"                                               />
                        <TextBlock Grid.Row="6"  Grid.Column="2" Style="{StaticResource AttributeValueTextBlockStyle}" Text="{Binding Ratings[6].Value, Converter={StaticResource Double2Converter}}" />

                    </Grid>
                </PivotItem>
            </Pivot>
            <TextBlock 
                Grid.Row="3" Grid.Column="0"
                Text="{Binding Converter={StaticResource PlayerToPositionStringConverter}}"
                Style="{StaticResource PlayerSubheaderTextBlockStyle}"
                HorizontalAlignment="Center"
                Margin="0,0,0,8" />
        </Grid>
    </Grid>
</m:Page_>
