                                                    <RadioButton Grid.Row="2"  Grid.Column="0" Content="Intrinsic Graeme Kelly" IsChecked="{Binding CA18ViewModes[1].IsChecked, Mode=TwoWay}" Margin="0,8,0,0" />
                                                    <RadioButton Grid.Row="4"  Grid.Column="0" Content="Intrinsic Normalized"   IsChecked="{Binding CA18ViewModes[2].IsChecked, Mode=TwoWay}" Margin="0,8,0,0" />
                                                    <RadioButton Grid.Row="6"  Grid.Column="0" Content="In Game"                IsChecked="{Binding CA18ViewModes[3].IsChecked, Mode=TwoWay}" Margin="0,8,0,0" />
                                                    <RadioButton Grid.Row="8"  Grid.Column="0" Content="In Match"               IsChecked="{Binding CA18ViewModes[4].IsChecked, Mode=TwoWay}" Margin="0,8,0,0" />
                                                    <RadioButton Grid.Row="10" Grid.Column="0" Content="In Match Normalized"    IsChecked="{Binding CA18ViewModes[5].IsChecked, Mode=TwoWay}" Margin="0,8,0,0" />
                                                    <TextBlock
                                                        Grid.Row="1" Grid.Column="0"
                                                        Text="Show the values as they appear in a save."
                                                        Style="{StaticResource CaptionTextBlockStyle}"
                                                        TextWrapping="Wrap" MaxWidth="400"
                                                        Foreground="{StaticResource SystemControlForegroundBaseMediumBrush}" />
                                                    <TextBlock
                                                        Grid.Row="3" Grid.Column="0"
                                                        Text="Show the values as they appear in the Graeme Kelly Save Game Editor."
                                                        Style="{StaticResource CaptionTextBlockStyle}"
                                                        TextWrapping="Wrap" MaxWidth="400"
                                                        Foreground="{StaticResource SystemControlForegroundBaseMediumBrush}" />
                                                    <TextBlock
                                                        Grid.Row="5" Grid.Column="0"
                                                        Text="Show the values normalized to the range 1-20."
                                                        Style="{StaticResource CaptionTextBlockStyle}"
                                                        TextWrapping="Wrap" MaxWidth="400"
                                                        Foreground="{StaticResource SystemControlForegroundBaseMediumBrush}" />
                                                    <TextBlock
                                                        Grid.Row="7" Grid.Column="0"
                                                        Text="Show the values as they appear in the game."
                                                        Style="{StaticResource CaptionTextBlockStyle}"
                                                        TextWrapping="Wrap" MaxWidth="400"
                                                        Foreground="{StaticResource SystemControlForegroundBaseMediumBrush}" />
                                                    <TextBlock
                                                        Grid.Row="9" Grid.Column="0"
                                                        Text="Show the values as they used by the match engine."
                                                        Style="{StaticResource CaptionTextBlockStyle}"
                                                        TextWrapping="Wrap" MaxWidth="400"
                                                        Foreground="{StaticResource SystemControlForegroundBaseMediumBrush}" />
                                                    <TextBlock
                                                        Grid.Row="11" Grid.Column="0"
                                                        Text="Show the in match values normalized to the range 1-20. The app always uses these values when calculating the ratings and applying the filters."
                                                        Style="{StaticResource CaptionTextBlockStyle}"
                                                        TextWrapping="Wrap" MaxWidth="400"
                                                        Foreground="{StaticResource SystemControlForegroundBaseMediumBrush}" />
                                                </Grid>
                                            </m:GroupBoxControl>
                                        </Grid>
                                    </PivotItem>
                                    <PivotItem
                                        Header="Attribute Columns">
                                        <ItemsControl
                                            ItemsSource="{Binding Columns}">
                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <ItemsWrapGrid Orientation="Vertical" MaximumRowsOrColumns="12" />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding IsChecked, Mode=TwoWay}">
                                                        <CheckBox.ContentTemplate>
                                                            <DataTemplate>
                                                                <TextBlock Text="{Binding Name}" Width="128" />
                                                            </DataTemplate>
                                                        </CheckBox.ContentTemplate>
                                                    </CheckBox>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </PivotItem>
                                    <PivotItem
                                        Header="Other Columns">
                                        <ItemsControl
                                            ItemsSource="{Binding Columns2}">
                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <ItemsWrapGrid Orientation="Vertical" MaximumRowsOrColumns="12" />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding IsChecked, Mode=TwoWay}">
                                                        <CheckBox.ContentTemplate>
                                                            <DataTemplate>
                                                                <TextBlock Text="{Binding Name}" Width="128" />
                                                            </DataTemplate>
                                                        </CheckBox.ContentTemplate>
                                                    </CheckBox>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </PivotItem>
                                </Pivot>
                            </Flyout>
                            </FlyoutBase.AttachedFlyout>
                            <i:Interaction.Behaviors>
                                <m:FlyoutBehavior IsOpened="{Binding SettingsViewModel.IsOpened, Mode=TwoWay}" />
                            </i:Interaction.Behaviors>
                    </m:AppBarButton>
                    <m:AppBarButton
                        Grid.Row="0" Grid.Column="7"
                        Label="About"
                        IsCompact="{Binding ElementName=CommandBar, Path=IsOpen, Converter={StaticResource InverseBooleanConverter}}"
                        Command="{Binding About}">
                        <m:AppBarButton.Icon>
                            <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE946;" />
                        </m:AppBarButton.Icon>
                        <FlyoutBase.AttachedFlyout>
                            <Flyout>
                                <Grid
                                    DataContext="{Binding AboutViewModel}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="32" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="32" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <Grid
                                        Grid.Row="0" Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="16" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Rectangle
                                            Grid.Row="0" Grid.Column="0" Grid.RowSpan="2"
                                            Fill="{ThemeResource SystemControlBackgroundAccentBrush}"
                                            Width="44" Height="44"
                                            VerticalAlignment="Center" />
                                        <Image
                                            Grid.Row="0" Grid.Column="0" Grid.RowSpan="2"
                                            Source="ms-appx:///Assets/Square44x44Logo.png"
                                            Width="44" Height="44"
                                            VerticalAlignment="Center" />
                                        <TextBlock
                                            Grid.Row="0" Grid.Column="2"
                                            Text="CM Scout Intrinsic"
                                            Style="{StaticResource SubtitleTextBlockStyle}" />
                                        <TextBlock
                                            Grid.Row="1" Grid.Column="2"
                                            Text="&#169; Vladimir Filatov, 2018" />
                                    </Grid>

                                    <Grid
                                        Grid.Row="2" Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="8" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="8" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="16" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>

                                        <TextBlock
                                            Grid.Row="0" Grid.Column="0"
                                            Text="App Version" />
                                        <TextBlock
                                            Grid.Row="0" Grid.Column="2"
                                            Text="{Binding AppVersion}" />

                                        <TextBlock
                                            Grid.Row="2" Grid.Column="0"
                                            Text="Website" />
                                        <HyperlinkButton
                                            Grid.Row="2" Grid.Column="2"
                                            Content="CM Scout Intrinsic"
                                            NavigateUri="http://www.runshoot.ru/en/cm_scout_intrinsic.html"
                                            Padding="0" >
                                        </HyperlinkButton>

                                        <TextBlock
                                            Grid.Row="4" Grid.Column="0"
                                            Text="Email" />
                                        <HyperlinkButton
                                            Grid.Row="4" Grid.Column="2"
                                            Content="info@runshoot.ru"
                                            NavigateUri="mailto:info@runshoot.ru?subject=CM%20Scout%20Intrinsic"
                                            Padding="0" />

                                    </Grid>
                                    <Button
                                        Grid.Row="4" Grid.Column="0"
                                        Style="{StaticResource RateTheAppButtonStyle}"
                                        Command="{Binding RateApp}"
                                        HorizontalAlignment="Center" />

                                </Grid>
                            </Flyout>
                        </FlyoutBase.AttachedFlyout>
                        <i:Interaction.Behaviors>
                            <m:FlyoutBehavior IsOpened="{Binding AboutViewModel.IsOpened, Mode=TwoWay}" />
                        </i:Interaction.Behaviors>
                    </m:AppBarButton>
                    
                    <TextBlock
                        Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="7" 
                        Text="{Binding OpenViewModel.OpenedFile.Name}"
                        Style="{StaticResource TitleTextBlockStyle}"
                        FontWeight="Bold"
                        Foreground="{ThemeResource SystemControlForegroundAccentBrush}"
                        HorizontalAlignment="Center" VerticalAlignment="Top"
                        Margin="48,8,0,0" />
                    
                </Grid>
            </CommandBar.Content>
        </CommandBar>
    </Page.TopAppBar>

    <Grid
        Background="{ThemeResource PageBackgroundThemeBrush}"
        Name="GridRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <m:DataGrid
            Grid.Row="0" Grid.Column="0"
            ItemsSource="{Binding Players}"
            SortColumn="{Binding PlayersSortColumn, Mode=TwoWay}"
            SelectionMode="Single"
            SelectedItem="{Binding SelectedPlayer, Mode=TwoWay}"
            ScrollFunction="{Binding ScrollToPlayer, Mode=TwoWay}"
            SetIsColumnVisibleFunction="{Binding SetIsColumnVisible, Mode=TwoWay}"
            CellTappedCommand="{Binding PlayerTapped}"
            CellRightTappedCommand="{Binding PlayerRightTapped}"
            Visibility="{Binding OpenViewModel.OpenedFile, Converter={StaticResource NullToVisibilityConverter}}">
            <m:DataGrid.Columns>
                
                <m:DataGridColumn ColumnName="Rating" Text="Rating" SizeMode="Fixed" Width="96" CanSort="True" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"  />
                                    <ColumnDefinition Width="48" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0" Grid.Column="0"
                                    Text="{Binding Row.Rating.Value, Converter={StaticResource Double2Converter}}"
                                    HorizontalAlignment="Right" VerticalAlignment="Center" />
                                <TextBlock
                                    Grid.Row="0" Grid.Column="1"
                                    Text="{Binding Row.Rating.Code}"
                                    HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Grid>
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>
                
                <m:DataGridColumn ColumnName="Name" Text="Name" SizeMode="Fixed" Width="144" CanSort="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition>
                                        <RowDefinition.Height>
                                            <Binding Path="Row.FirstName">
                                                <Binding.Converter>
                                                    <m:ConditionConverter Condition="==" OnRight="" IfTrue="Auto" IfFalse="*" />
                                                </Binding.Converter>
                                            </Binding>
                                        </RowDefinition.Height>
                                    </RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0" Grid.Column="0"
                                    Text="{Binding Row.SecondName}"
                                    VerticalAlignment="Center"
                                    Margin="8,0,8,0" />
                                <TextBlock
                                    Grid.Row="1" Grid.Column="0"
                                    Text="{Binding Row.FirstName}"
                                    VerticalAlignment="Center"
                                    Margin="8,0,8,0"
                                    Foreground="Gray"
                                    Visibility="{Binding RelativeSource={RelativeSource Self}, Path=Text, Converter={StaticResource EmptyToVisibilityConverter}}" />
                            </Grid>
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>
                
                <m:DataGridColumn ColumnName="Nation" Text="Nation" SizeMode="Fixed" Width="80" CanSort="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition>
                                        <RowDefinition.Height>
                                            <Binding Path="Row.SecondNationCode">
                                                <Binding.Converter>
                                                    <m:ConditionConverter Condition="==" OnRight="" IfTrue="Auto" IfFalse="*" />
                                                </Binding.Converter>
                                            </Binding>
                                        </RowDefinition.Height>
                                    </RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0" Grid.Column="0"
                                    Text="{Binding Row.FirstNationCode}"
                                    VerticalAlignment="Center"
                                    Margin="8,0,8,0" />
                                <TextBlock
                                    Grid.Row="1" Grid.Column="0"
                                    Text="{Binding Row.SecondNationCode}"
                                    VerticalAlignment="Center"
                                    Margin="8,0,8,0"
                                    Visibility="{Binding RelativeSource={RelativeSource Self}, Path=Text, Converter={StaticResource EmptyToVisibilityConverter}}" />
                            </Grid>
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>
                
                <m:DataGridColumn ColumnName="Club" Text="Club" SizeMode="Fixed" Width="192" CanSort="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0" Grid.Column="0"
                                    Text="{Binding Row.ClubShortName}"
                                    VerticalAlignment="Center"
                                    Margin="8,0,8,0" />
                                <TextBlock
                                    Grid.Row="1" Grid.Column="0"
                                    Text="{Binding Row.ClubNationCode}"
                                    VerticalAlignment="Center"
                                    Foreground="Gray"
                                    Margin="8,0,8,0" />
                            </Grid>
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Position" Text="Position" SizeMode="Fixed" Width="112" CanSort="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row, Converter={StaticResource PlayerToPositionShortStringConverter}}"
                                VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Age" Text="Age" SizeMode="Fixed" Width="64" CanSort="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.Age}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Current Ability" Text="CA" SizeMode="Fixed" Width="64" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.CurrentAbility}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Potential Ability" Text="PA" SizeMode="Fixed" Width="64" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.PotentialAbility}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Value" Text="Value" SizeMode="Fixed" Width="104" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.Value_, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0}'}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Wage" Text="Wage" SizeMode="Fixed" Width="88" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.Contract.Wage, Converter={StaticResource StringFormatConverter}, ConverterParameter='{}&#x00A3;{0:N0}'}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Current Reputation" Text="C Rep" SizeMode="Fixed" Width="80" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.CurrentReputation}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Home Reputation" Text="H Rep" SizeMode="Fixed" Width="80" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.HomeReputation}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="World Reputation" Text="W Rep" SizeMode="Fixed" Width="80" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.WorldReputation}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Left Foot" Text="LF" SizeMode="Fixed" Width="56" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.LeftFoot}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Right Foot" Text="RF" SizeMode="Fixed" Width="56" CanSort="True" IsVisible="False" IsDefaultSortDirectionDescending="True">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Row.RightFoot}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn
                    ColumnName="Attribute"
                    ChildPropertyPath="Attributes"
                    SizeMode="Fixed"
                    Width="64"
                    CanSort="True"
                    IsVisible="False"
                    IsDefaultSortDirectionDescending="True"
                    HeaderItemsSource="{Binding Attributes}">
                    <m:DataGridColumn.HeaderStyle>
                        <Style TargetType="m:DataGridColumnHeaderItem">
                            <Setter Property="ContentTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <TextBlock
                                            Text="{Binding Column.Item.Code}"
                                            HorizontalAlignment="Left" VerticalAlignment="Center"
                                            Margin="8,0,8,0" />
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </m:DataGridColumn.HeaderStyle>
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <TextBlock
                                Text="{Binding Child.Value}"
                                HorizontalAlignment="Right" VerticalAlignment="Center"
                                Margin="8,0,8,0" />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>

                <m:DataGridColumn ColumnName="Stretch" SizeMode="Stretch" CanSort="False">
                    <m:DataGridColumn.CellContentTemplate>
                        <DataTemplate>
                            <Grid />
                        </DataTemplate>
                    </m:DataGridColumn.CellContentTemplate>
                </m:DataGridColumn>
                
            </m:DataGrid.Columns>
        </m:DataGrid>
        
        <Grid
            Grid.Row="1" Grid.Column="0"
            Height="64"
            Background="{ThemeResource AppBarBackground}"
            DataContext="{Binding QuickFilterViewModel}"
            Visibility="{Binding ElementName=GridRoot, Path=DataContext.OpenViewModel.OpenedFile, Converter={StaticResource NullToVisibilityConverter}}">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            
            <Grid
                Grid.Row="0" Grid.Column="0"
                Margin="16,0,0,0"
                Visibility="{Binding AppliedFilterName, Converter={StaticResource NullToVisibilityConverter}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
                    Text="{Binding AppliedFilterName}"
                    Foreground="{ThemeResource SystemControlForegroundAccentBrush}"
                    VerticalAlignment="Center"
                    Style="{StaticResource BodyTextBlockStyle}" />
                <TextBlock
                    Grid.Row="1" Grid.Column="0"
                    Text="{Binding ElementName=GridRoot, Path=DataContext.PlayersCount}"
                    VerticalAlignment="Center"
                    Style="{StaticResource BodyTextBlockStyle}" />
                <TextBlock
                    Grid.Row="1" Grid.Column="1"
                    Text="players selected"
                    VerticalAlignment="Center"
                    Margin="4,0,0,0"
                    Style="{StaticResource BodyTextBlockStyle}" />
            </Grid>

            <m:GroupBoxControl
                Grid.Row="0" Grid.Column="2"
                Header="Name"
                VerticalAlignment="Center"
                Margin="0,0,32,0">
                <TextBox
                    Text="{Binding PlayerName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                    Width="128" />
            </m:GroupBoxControl>

            <m:GroupBoxControl
                Grid.Row="0" Grid.Column="3"
                Header="Side"
                VerticalAlignment="Center"
                Margin="0,0,32,0">
                <ItemsControl
                    ItemsSource="{Binding Sides}"
                    Margin="-4,0,0,0">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Horizontal" />
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <ToggleButton
                                IsChecked="{Binding IsChecked, Mode=TwoWay}"
                                Content="{Binding Item.Code, TargetNullValue='&#x2211;'}"
                                Padding="0"
                                Width="32" Height="32"
                                Margin="4,0,0,0" />
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </m:GroupBoxControl>

            <m:GroupBoxControl
                Grid.Row="0" Grid.Column="4"
                Header="Position"
                VerticalAlignment="Center"
                Margin="0,0,32,0">
                <ItemsControl
                    ItemsSource="{Binding Positions}"
                    Margin="-4,0,0,0">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Horizontal" />
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <ToggleButton
                                IsChecked="{Binding IsChecked, Mode=TwoWay}"
                                Content="{Binding Item.Code, TargetNullValue='&#x2211;'}"
                                Padding="0"
                                Width="32" Height="32"
                                Margin="4,0,0,0" />
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </m:GroupBoxControl>

            <m:GroupBoxControl
                Grid.Row="0" Grid.Column="5"
                Header="Rating"
                VerticalAlignment="Center"
                Margin="0,0,16,0">
                <Button
                    Content="{Binding SelectedRatingPosition.Code}"
                    Padding="0"
                    Width="32" Height="32">
                    <Button.Flyout>
                        <MenuFlyout>
                            <ToggleMenuFlyoutItem
                                Text="{Binding RatingPositions[0].Item.Name}"
                                IsChecked="{Binding RatingPositions[0].IsChecked, Mode=TwoWay}"
                                IsEnabled="{Binding RelativeSource={RelativeSource Self}, Path=IsChecked, Converter={StaticResource InverseBooleanConverter}}" />
                            <ToggleMenuFlyoutItem
